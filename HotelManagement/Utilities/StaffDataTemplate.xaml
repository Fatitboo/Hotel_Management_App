<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:RoomCatalogView="clr-namespace:HotelManagement.View.Staff.RoomCatalogManagement"
                    xmlns:RoomCatalogVM="clr-namespace:HotelManagement.ViewModel.StaffVM.RoomCatalogManagementVM"
                    xmlns:TroubleRpView="clr-namespace:HotelManagement.View.Staff.TroubleReport"
                    xmlns:TroubleRpVM="clr-namespace:HotelManagement.ViewModel.StaffVM.TroubleReportVM"
                    >

    <DataTemplate DataType="{x:Type RoomCatalogVM:RoomCatalogManagementVM}">
        <RoomCatalogView:RoomCatalogManagementPage></RoomCatalogView:RoomCatalogManagementPage>
    </DataTemplate>
    <DataTemplate DataType="{x:Type TroubleRpVM:TroubleReportVM}">
        <TroubleRpView:TroubleReportPage></TroubleRpView:TroubleReportPage>
    </DataTemplate>
</ResourceDictionary>